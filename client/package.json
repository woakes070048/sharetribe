{
  "repository": "sharetribe/sharetribe",
  "license": "MIT",
  "engines": {
    "node": "6.1"
  },
  "scripts": {
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint .",
    "stylelint": "stylelint **/*.css",
    "build:client": "webpack --config webpack.client.config.js",
    "build:server": "webpack --config webpack.server.config.js",
    "build:production:client": "NODE_ENV=production webpack --config webpack.client.config.js",
    "build:production:server": "NODE_ENV=production webpack --config webpack.server.config.js",
    "build:production": "npm run build:production:client && npm run build:production:server",
    "build:dev:client": "webpack -w --config webpack.client.config.js",
    "build:dev:server": "webpack -w --config webpack.server.config.js",
    "build:test": "npm run build:client && npm run build:server",
    "print-phantomjs-version": "phantomjs --version",
    "start-phantomjs": "phantomjs --webdriver=8910",
    "styleguide": "start-storybook -p 9001",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "mocha './app/components/**/*.story.js' --reporter mocha-circleci-reporter --require babel-register --require ignore-styles --require ./mochaConfig.js"
  },
  "dependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.13.0",
    "babel-runtime": "6.11.6",
    "classnames": "2.2.5",
    "css-loader": "0.23.1",
    "customfile-loader": "./customloaders/customfileloader",
    "es6-shim": "0.35.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "i18n-js": "http://github.com/sharetribe/i18n-js/archive/interpolation-mode.tar.gz",
    "immutable": "3.8.1",
    "imports-loader": "0.6.5",
    "json-loader": "https://registry.npmjs.org/json-loader/-/json-loader-0.5.4.tgz",
    "loader-utils": "0.2.15",
    "lodash": "4.15.0",
    "node-libs-browser": "1.0.0",
    "node-sass": "3.8.0",
    "numbro": "^1.9.3",
    "postcss-cssnext": "2.7.0",
    "postcss-loader": "0.10.0",
    "postcss-mixins": "5.0.1",
    "r-dom": "2.2.3",
    "raw-loader": "0.5.1",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-on-rails": "6.0.5",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "resolve-url-loader": "1.6.0",
    "style-loader": "0.13.1",
    "transit-js": "^0.8.846",
    "webpack": "1.13.1",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "2.13.0",
    "@kadira/storybook-deployer": "github:mporkola/storybook-deployer",
    "babel-eslint": "6.1.2",
    "babel-plugin-react-transform": "2.0.2",
    "babel-register": "6.14.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.5.1",
    "enzyme": "2.4.1",
    "eslint": "3.3.1",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-react": "6.1.1",
    "ignore-styles": "5.0.1",
    "json": "9.0.4",
    "mocha": "3.0.2",
    "mocha-circleci-reporter": "0.0.2",
    "phantomjs-prebuilt": "2.1.12",
    "react-addons-test-utils": "15.3.1",
    "react-transform-hmr": "1.0.4",
    "storybook-addon-specifications": "1.0.15",
    "stylelint": "7.1.0",
    "stylelint-config-standard": "12.0.0",
    "webpack-dev-server": "1.14.1"
  },
  "browser": {
    "fs": false
  }
}
